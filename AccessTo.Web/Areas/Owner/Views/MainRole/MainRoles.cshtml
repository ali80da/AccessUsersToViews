@model List<AccessToAuth.Data.Entities.MainRoleCookie.MainRole>
@{
    ViewData["Title"] = "Main Roles";
}


<h1>@ViewData["Title"]</h1>


<div class="btn-group mt-4 mb-4">
    <a asp-action="RoleValidationGuid" class="btn btn-primary">RoleValidationGuid</a>
</div>


<div class="container">
    @if (!Model.Any())
    {
        <p>No Role Found.</p>
    }
    else
    {

        for (int i = 0; i < Model.Count; i++)
        {
            <div class="card mt-2">
                <div class="card-header">
                    MainRole Key : <b>@Model[i].Key</b>
                </div>
                <div class="card-body">
                    MainRole Value : <b>@Model[i].Value</b>
                </div>
                <div class="card-footer">
                    Last Time Value Changed = @Model[i].LastValidCookie
                </div>
            </div>
        }
    
    }
</div>





