@model AccessTo.Web.Main.DataMo.MainRole.RoleValidationGuidDataMo
@{
    ViewData["Title"] = "RoleValidationGuid";
}

<h1>RoleValidationGuid</h1>

<form asp-area="Owner" asp-controller="MainRole" asp-action="RoleValidationGuid" method="POST">
    <div asp-validation-summary="ModelOnly"></div>

    <div class="form-group">
        <label class="control-label" asp-for="@Model.Value"></label>
        <input readonly="readonly" class="form-control" asp-for="@Model.Value" />
    </div>
    <div class="form-group">
        <label class="control-label" asp-for="@Model.LastValidCookie"></label>
        <input readonly="readonly" class="form-control" asp-for="@Model.LastValidCookie" />
    </div>
    <br />
    <button class="btn btn-primary" type="submit">Generate New Guid</button>
</form>